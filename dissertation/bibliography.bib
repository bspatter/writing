journal = {J. Ultrasound Med.},
month = {jul},
number = {7},
pages = {873--882},
title = {{Superthreshold Behavior of Ultrasound-Induced Lung Hemorrhage in Adult Rats: Role of Pulse Repetition Frequency and Pulse Duration}},
