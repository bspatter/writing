publisher = {Acoustical Society of America},
title = {{Cavitation mediated rat lung bioeffects from diagnostic ultrasound}},
url = {http://scitation.aip.org/content/asa/journal/jasa/109/5/10.1121/1.4744613},
volume = {109},
year = {2001}
}
