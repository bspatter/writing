pmid = {9847287},
publisher = {J. Appl. Physiol},
title = {{Pulmonary capillary stress failure}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/11090605 http://www.atsjournals.org/doi/abs/10.1164/ajrccm.158.6.9804076 http://jap.physiology.org/content/89/6/2483.abstract},
volume = {89},
