isbn = {0-7803-6365-5},
issn = {1051-0117},
journal = {2000 IEEE Ultrasonics Symposium. Proceedings. An International Symposium (Cat. No.00CH37121)},
keywords = {lung hemorrhage,mouse lung,pulsed ultrasound,rat lung,ultrasound bioeffects},
number = {2},
pages = {267--277},
title = {{Role of pulse repetition frequency and exposure duration on the
superthreshold behavior of ultrasound-induced lung haemorrhage in adult
mice and rats}},
volume = {2},
year = {2000}
}
