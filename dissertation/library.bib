@article{Patterson1976,
abstract = {Human beings have never been able to live without emotional support and material help either singly or in groups. This fact becomes more obvious as we look around the world today. Everybody wants to be loved, cared for, and to experience the essence of relationships of one kind and another. The midwife's opportunity to give this support is clearly seen, as she is the closest person to the mother, baby and family as a whole, and at the time when her help is needed. Her positive attitude towards breast-feeding can help to create the socially supportive situation which is important to the mother, father, baby and the family as a whole. The midwife may begin to get across her ideas long before parenthood becomes a reality. She needs to be a part of the community, and close liaison with the following groups would help to give the right start to promote the necessary material and emotional support: youth leaders of the community; church groups; schools; clubs and sports centres; the male sector of the community; organizers of advertisements, e.g. public transport, television and news media; medical/nursing/paramedical colleagues; and the community as a whole. After that preparatory phase, the intermediate phase needs to be considered: during the pre-natal period, as much support as well as adequate instructions must be given. In the final phase, after delivery, the encouragement previously given affords the chance for breast-feeding to start in the delivery room. During post-natal life, the support continues for as long as possible. Weaning will follow, any time from four months to one year. We must remember the fringe benefits of breast-feeding: natural family planning; emotional comfort; nursing experience between the mother and the baby; and, finally, breast milk is the perfect food.},
author = {Patterson, B},
isbn = {0300-5208 (Print)$\backslash$r0300-5208 (Linking)},
journal = {Ciba Foundation symposium},
number = {45},
pages = {205--214},
pmid = {1052048},
title = {{Every life needs nutritional and emotional satisfaction: the midwife's opportunity.}},
year = {1976}
}
@article{OBrien2007,
abstract = {Ultrasonic biophysics is the study of mechanisms responsible for how ultrasound and biological materials interact. Ultrasound-induced bioeffect or risk studies focus on issues related to the effects of ultrasound on biological materials. On the other hand, when biological materials affect the ultrasonic wave, this can be viewed as the basis for diagnostic ultrasound. Thus, an understanding of the interaction of ultrasound with tissue provides the scientific basis for image production and risk assessment. Relative to the bioeffect or risk studies, that is, the biophysical mechanisms by which ultrasound affects biological materials, ultrasound-induced bioeffects are generally separated into thermal and non-thermal mechanisms. Ultrasonic dosimetry is concerned with the quantitative determination of ultrasonic energy interaction with biological materials. Whenever ultrasonic energy is propagated into an attenuating material such as tissue, the amplitude of the wave decreases with distance. This attenuation is due to either absorption or scattering. Absorption is a mechanism that represents that portion of ultrasonic wave that is converted into heat, and scattering can be thought of as that portion of the wave, which changes direction. Because the medium can absorb energy to produce heat, a temperature rise may occur as long as the rate of heat production is greater than the rate of heat removal. Current interest with thermally mediated ultrasound-induced bioeffects has focused on the thermal isoeffect concept. The non-thermal mechanism that has received the most attention is acoustically generated cavitation wherein ultrasonic energy by cavitation bubbles is concentrated. Acoustic cavitation, in a broad sense, refers to ultrasonically induced bubble activity occurring in a biological material that contains pre-existing gaseous inclusions. Cavitation-related mechanisms include radiation force, microstreaming, shock waves, free radicals, microjets and strain. It is more challenging to deduce the causes of mechanical effects in tissues that do not contain gas bodies. These ultrasonic biophysics mechanisms will be discussed in the context of diagnostic ultrasound exposure risk concerns. ?? 2006 Elsevier Ltd. All rights reserved.},
author = {O'Brien, William D. and Miller, Douglas L.},
doi = {10.1016/j.pbiomolbio.2006.07.010},
file = {:home/awesome/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Miller - 2007 - Overview of experimental studies of biological effects of medical ultrasound caused by gas body activation and inertial.pdf:pdf;:home/awesome/Documents/Mendeley/O'Brien, Miller/O'Brien, Miller - 2007 - Ultrasound-biophysics mechanisms - Progress in Biophysics and Molecular Biology.pdf:pdf},
isbn = {0079-6107},
issn = {00796107},
journal = {Progress in Biophysics and Molecular Biology},
keywords = {Acoustic cavitation,Lithotripsy,Myocardial contrast echocardiography,Non-thermal mechanism,Thermal mechanism,Ultrasonic bioeffects,Ultrasonic biophysics,Ultrasonic dosimetry,Ultrasound adverse effects,Ultrasound contrast agents},
month = {jan},
number = {1-3},
