month = {may},
number = {2093},
pages = {1125--1140},
title = {{On the impulsive generation of drops at the interface of two inviscid fluids}},
url = {http://rspa.royalsocietypublishing.org/cgi/doi/10.1098/rspa.2007.0308},
volume = {464},
