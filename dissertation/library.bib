journal = {Physics in medicine and biology},
keywords = {Acoustics,Animals,Blood Coagulation,Cattle,High-Intensity Focused Ultrasound Ablation,High-Intensity Focused Ultrasound Ablation: instru,High-Intensity Focused Ultrasound Ablation: method,Liver,Liver: cytology,Liver: surgery,Swine},
month = {dec},
